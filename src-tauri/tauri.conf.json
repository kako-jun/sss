{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "sss",
  "mainBinaryName": "sss",
  "version": "1.0.0",
  "identifier": "com.kakojun.sss",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": true,
        "resizable": true,
        "maximized": true,
        "title": "sss - Smart Slide Show",
        "width": 1920,
        "height": 1080,
        "decorations": false,
        "alwaysOnTop": false,
        "visible": true,
        "backgroundColor": "#000000",
        "useHttpsScheme": true
      }
    ],
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "csp": null
    }
  }
}